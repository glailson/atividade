<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">

	<managed-bean>
		<managed-bean-name>paginaPrincipalBean</managed-bean-name>
		<managed-bean-class>br.com.atividade.controller.PaginaPrincipalBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>clinicaBean</managed-bean-name>
		<managed-bean-class>br.com.atividade.controller.ClinicaBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>leitoBean</managed-bean-name>
		<managed-bean-class>br.com.atividade.controller.LeitoBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>leitoInternacaoBean</managed-bean-name>
		<managed-bean-class>br.com.atividade.controller.LeitoInternacaoBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>pacienteBean</managed-bean-name>
		<managed-bean-class>br.com.atividade.controller.PacienteBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<display-name>principal</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>principal</from-outcome>
			<to-view-id>/paginaPrincipal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
